<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.24"/>
  <object class="GtkWindow" id="main_window">
    <property name="can-focus">False</property>
    <property name="title" translatable="yes">Мой Glade Интерфейс</property>
    <property name="default-width">400</property>
    <property name="default-height">250</property>
    <property name="destroy-with-parent">False</property>
    <property name="type-hint">normal</property>
    <property name="gravity">north-west</property>
    <child>
      <object class="GtkFixed">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <child>
          <object class="GtkButton" id="btn_click_me">
            <property name="label" translatable="yes">Нажми меня!</property>
            <property name="width-request">150</property>
            <property name="height-request">50</property>
            <property name="visible">True</property>
            <property name="can-focus">True</property>
            <property name="receives-default">True</property>
            <signal name="clicked" handler="on_btn_click_me_clicked" swapped="no"/>
          </object>
          <packing>
            <property name="x">125</property>
            <property name="y">100</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>